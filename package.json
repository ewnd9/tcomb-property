{
  "name": "tcomb-property",
  "description": "Alternative syntax for defining default props in tcomb structures",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel -d dist src",
    "build:watch": "babel -w -d dist src",
