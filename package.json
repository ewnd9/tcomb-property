    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.0.0",
    "husky": "^0.11.6",
    "tcomb": "^3.2.15"
  },
  "author": "ewnd9 <ewndnine@gmail.com>",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/tcomb-property.git"
  },
  "peerDependencies": {
    "tcomb": "^3.2.15"
  },
  "ava": {
